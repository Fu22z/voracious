{
  "name": "Voracious",
  "description": "A tool for foreign language learners to watch videos, read comics, and more.",
  "author": "Russel Simmons",
  "license": "MIT",
  "version": "0.1.0",
  "repository": "rsimmons/voracious",
  "private": true,
  "dependencies": {
    "diff-match-patch": "^1.0.0",
    "franc": "^3.1.1",
    "immutable": "^3.8.1",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-infinite": "^0.12.1",
    "react-router-dom": "^4.1.2",
    "react-scripts": "1.0.10",
    "reselect": "^3.0.1"
  },
  "devDependencies": {
    "electron": "^1.7.5",
    "electron-builder": "^19.24.1",
    "react-addons-perf": "^15.4.2"
  },
  "homepage": "./",
  "main": "src/electron-starter.js",
  "scripts": {
    "react-start": "BROWSER=none react-scripts start",
    "electron": "ELECTRON_START_URL=http://localhost:3000 electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "react-build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "build": {
    "appId": "org.rsimmons.voracious",
    "directories": {
      "buildResources": "public"
    }
  },
  "postinstall": "electron-builder install-app-deps"
}

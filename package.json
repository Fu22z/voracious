{
  "name": "immersion-player",
  "description": "A tool for foreign language learners to watch videos, read comics, and more.",
  "version": "0.0.0",
  "repository": "rsimmons/immersion-player",
  "main": "index.js",
  "scripts": {
    "lint": "eslint index.js actions/** components/** reducers/** util/** || true",
    "watch": "NODE_ENV=development watchify --debug --extension=js -v -o bundle.js index.js"
  },
  "author": "Russel Simmons",
  "license": "MIT",
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "es2015",
            "react"
          ],
          "plugins": [
            "transform-object-rest-spread"
          ]
        }
      ],
      [
        "envify",
        {}
      ]
    ]
  },
  "dependencies": {
    "escape-html": "^1.0.3",
    "immutable": "^3.8.1",
    "react": "^15.0.2",
    "react-dom": "^15.0.2",
    "react-redux": "^4.4.5",
    "redux": "^3.5.2",
    "redux-logger": "^2.6.1",
    "redux-thunk": "^2.1.0"
  },
  "devDependencies": {
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babelify": "^7.3.0",
    "browserify": "^13.0.1",
    "envify": "^3.4.0",
    "eslint": "^2.11.1",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-import": "^1.8.1",
    "eslint-plugin-jsx-a11y": "^1.3.0",
    "eslint-plugin-react": "^5.1.1",
    "watchify": "^3.7.0"
  }
}

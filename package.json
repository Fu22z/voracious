{
  "name": "voracious",
  "description": "A tool for foreign language learners to watch videos, read comics, and more.",
  "version": "0.0.0",
  "repository": "rsimmons/voracious",
  "main": "index.js",
  "scripts": {
    "lint": "eslint index.js actions/** components/** reducers/** util/** || true",
    "watch": "NODE_ENV=development watchify --debug --extension=js -v -o bundle.js index.js",
    "build": "NODE_ENV=production browserify --extension=js index.js | uglifyjs -c > bundle.js"
  },
  "author": "Russel Simmons",
  "license": "MIT",
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "es2015",
            "react"
          ],
          "plugins": [
            "transform-object-rest-spread",
            "transform-class-properties"
          ]
        }
      ],
      [
        "envify",
        {}
      ]
    ]
  },
  "dependencies": {
    "diff-match-patch": "^1.0.0",
    "escape-html": "^1.0.3",
    "immutable": "^3.8.1",
    "react": "^15.3.2",
    "react-dom": "^15.0.2",
    "reselect": "^2.5.4"
  },
  "devDependencies": {
    "babel-plugin-transform-class-properties": "^6.16.0",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babelify": "^7.3.0",
    "browserify": "^13.0.1",
    "envify": "^3.4.0",
    "eslint": "^2.11.1",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-import": "^1.8.1",
    "eslint-plugin-jsx-a11y": "^1.3.0",
    "eslint-plugin-react": "^5.1.1",
    "react-addons-shallow-compare": "^15.3.2",
    "uglify-js": "^2.6.4",
    "watchify": "^3.7.0"
  }
}
